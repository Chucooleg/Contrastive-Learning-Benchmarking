{
    "seed": 42,
    "simple_shatter_query_order": "random",

    "batch_size": 512,
    "max_epochs": 10000000000,
    "val_every_n_epoch": 1000,

    "d_model": 512,
    "embed_dropout": 0.0,
    "vec_repr": 512,

    "model": "generative",

    "embedding_by_property": true,
    "encoder": "transformer",
    "decoder": "transformer",
    "d_ff":1024,
    "num_heads":16,
    "N_enc":16,
    "N_dec":16,
    "attn_wt_tying_scheme": "untie_QKVO",
    "attn_wt_dropout": 0.0,
    "heads_dropout": 0.0,
    "pff_dropout": 0.0,
    "representation_pos": 0,

    "dotproduct_bottleneck": true,
    "normalize_dotproduct": true,
    "contrastive_use_infoNCE": true,
    "loss_temperature_const": 1.0,
    "loss_smoothing_const": 0.1,
    "nonlinear_classifier_scale_down_factor": [2,1],

    "contrastive_optimizer": "adam",

    "adam_lr": 1e-06,
    "adam_beta1": 0.9,
    "adam_beta2": 0.999,
    "adam_epsilon": 1e-08,
    "adam_weight_decay":0,

    "sgd_lr": 0.001,
    "sgd_momentum":0,

    "scheduled_adam_beta1": 0.9,
    "scheduled_adam_beta2": 0.98,
    "scheduled_adam_epsilon": 1e-09,
    "scheduled_adam_warmup_steps": 8000,

    "additional_lr_decay": true,
    "additional_lr_decay_gamma": 0.9,
    "decay_lr_starts": 8000,
    "decay_lr_stops": 36000,
    "decay_lr_interval": 100,

    "gradient_clip_val": 0,

    "debug":false,
    "extra_monitors":false
}